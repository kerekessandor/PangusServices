@model PangusServices.Models.Anvelope

@{
    ViewBag.Title = "Anvelope noi";
}

@using(Html.BeginForm())
{
    @Html.ValidationSummary()
    @Html.AntiForgeryToken()
    
    <div class="form-group">
        <label>Denumire</label>
        @Html.TextBoxFor(x => x.Marca, new { @class="form-control"})
    </div>
    
    <input type="submit" value="Salveaza" class="btn btn-primary" />
    <a href="@Url.Action("Index")" class="btn btn-default">Inapoi</a>
    
}






