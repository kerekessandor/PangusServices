@model PangusServices.Models.dimType

@{
    ViewBag.Title = "Edit";
}

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.HiddenFor(x => x.dimTypeID)

    <div class="row">
        <div class="form-group-sm col-lg-4">
            <label>Denumire</label>
            <b>@Html.TextBoxFor(x => x.Name, new { @class = "form-control" })</b>
        </div>
    </div>
    <br />
    <div class="form-group">
        <input type="submit" value="Salvare" class="btn btn-primary" />
        <a href="@Url.Action("Index")" class="btn btn-default">Inapoi</a>
    </div>
}

